{
	"name": "sveltekit-template",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --plugin prettier-plugin-svelte . --plugin prettier-plugin-tailwindcss . --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test"
	},
	"devDependencies": {
		"@iconify-json/lucide": "^1.2.13",
		"@playwright/test": "^1.45.3",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/eslint": "^9.6.0",
		"@types/node": "^22.9.0",
		"autoprefixer": "^10.4.20",
		"embla-carousel-svelte": "^8.5.1",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"postcss": "^8.4.47",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.7",
		"prettier-plugin-tailwindcss": "^0.6.8",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"sveltekit-flash-message": "^2.4.4",
		"tailwindcss": "^3.4.14",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.0.3",
		"vitest": "^2.0.4",
		"@inlang/paraglide-js": "1.11.3"
	},
	"dependencies": {
		"@iconify/json": "^2.2.270",
		"@pinecone-database/pinecone": "^4.0.0",
		"@sveltejs/adapter-vercel": "^5.4.7",
		"clsx": "^2.1.1",
		"iconify-icon": "^2.1.0",
		"marked": "^15.0.0",
		"svelte-sonner": "^0.3.28",
		"tailwind-merge": "^2.5.4",
		"tailwind-variants": "^0.2.1",
		"@inlang/paraglide-sveltekit": "0.12.1"
	}
}
